pipeline {
  agent any
  stages {
    stage('1') {
      steps {
        sh 'ls -al'
      }
    }
    stage('2') {
      steps {
        sh 'ls'
      }
    }
    stage('3') {
      steps {
        sh 'echo "Test"'
      }
    }
    stage('4') {
      steps {
        echo 'halo'
      }
    }
  }
}